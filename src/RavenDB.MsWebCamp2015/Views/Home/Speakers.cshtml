@model RavenDB.MsWebCamp2015.Models.HomeSpeakersViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div>
	<h1>@Model.Count speakers</h1>
	<ul>
		@foreach (var speaker in @Model.Speakers)
		{
            <li>@speaker.Name, @foreach (var tag in speaker.Tags)
			{ <a asp-controller="Home" asp-action="speakers" asp-route-tag="@tag">@tag </a> }  </li>
        }
	</ul>
</div>